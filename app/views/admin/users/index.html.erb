<h1>admin_user_index</h1>

<h3><%=link_to 'ユーザー作成' ,new_admin_user_path%></h3>



<table class="table">
  <tr>
    <th>権限</th>
    <th>名前</th>
    <th>メールアドレス</th>
    <th>タスク数</th>
    <th>詳細</th>
    <th>編集</th>
    <th>削除</th>
  </tr>
<% @user.each do |user| %>
  <tr>
    <td><%=user.admin%></td>
    <td><%=user.name%></td>
    <td><%=user.email%></td>
    <td><%=user.tasks.count%></td>
    <td><%=link_to '詳細' ,admin_user_path(user.id)%></td>
    <td><%=link_to '編集' ,edit_admin_user_path(user.id)%></td>
    <td><%=link_to '削除' ,admin_user_path(user.id),method: :delete,data:{confirm:'削除します'}%></td>
  </tr>
<% end %>
</table>
